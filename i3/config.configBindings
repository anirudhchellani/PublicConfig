#Open config file
#i3 
bindsym $mod+Control+i exec i3-sensible-terminal -e "vim ~/.config/i3/config"
#vim
bindsym $mod+Control+v exec i3-sensible-terminal -e "vim ~/.vimrc"
#i3blocks
bindsym $mod+Control+b exec i3-sensible-terminal -e "vim ~/.i3blocks.conf"
#Bash Profile
bindsym $mod+Control+p exec i3-sensible-terminal -e "vim ~/.profile"

